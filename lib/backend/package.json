    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "10.4.20",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "10.4.20",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/serve-static": "4.0.2",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/throttler": "^6.4.0",
    "argon2": "^0.41.1",
    "axios": "^1.13.1",
    "bcryptjs": "^3.0.2",
    "cache-manager": "^5.6.3",
    "cache-manager-ioredis-yet": "^1.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "ioredis": "^5.4.1",
    "keyv": "^4.5.4",
    "luxon": "^2.5.2",
    "nestjs-pino": "^4.3.0",
    "nodemailer": "^7.0.6",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^9.3.2",
