{
  "name": "vethome-backend",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "db:seed": "ts-node -r tsconfig-paths/register src/prisma/seed.ts",
    "test": "jest",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "smoke:chromium": "node -e \"const {chromium}=require('playwright'); chromium.launch({headless:true,args:['--no-sandbox','--disable-dev-shm-usage']}).then(b=>b.close().then(()=>console.log('Chromium OK'))).catch(e=>{console.error(e);process.exit(1)})\""
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.888.0",
    "@aws-sdk/s3-request-presigner": "^3.888.0",
