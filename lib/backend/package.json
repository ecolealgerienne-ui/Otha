    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.12",
    "@types/luxon": "^3.7.1",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.17",
    "@types/nodemailer": "^7.0.1",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.11.1",
